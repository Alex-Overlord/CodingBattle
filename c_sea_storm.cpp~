#include <iostream>
#include <iomanip>
#include <string>
using namespace std;

int main () 
{
  int pont, nb;
  cin >> pont >> nb;
  
  int* pos = new int[nb];
  //int* rayon = new int[nb];

  for (int i=0 ; i<nb ; i++) {
    cin >> pos[i];
  }
  for (int i=0 ; i<nb ; i++) {
    cout << pos[i];
  }

}
